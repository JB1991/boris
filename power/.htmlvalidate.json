{
    "extends": [
        "html-validate:recommended",
        "html-validate:document"
    ],
    "rules": {
        "attr-case": "off",
        "element-name": "off",
        "no-inline-style": "warn",
        "prefer-native-element": "warn",
        "text-content": "warn",
        "missing-doctype": "off",
        "no-missing-references": "off",
        "heading-level": "warn",
        "input-missing-label": "warn"
    }
}
